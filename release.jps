#!groovy
stage('Build') {
    node {
       build job: 'MyProject Build', parameters: [string(name: 'Branch', value: "${params.Branch}")]
    }
}
stage('QA') {
    when { 
        expression { return params.Environment ==~ /(?i)(QA|UAT|PROD)/ } 
    }
    node {
       build job: 'MyProject Deploy', parameters: [string(name: 'Environment', value: 'QA')]
    }
}
