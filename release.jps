#!groovy
pipeline {
    agent any
    stages {
        stage ('Build') {
            steps {
                echo "Build ..."
            }
        }
        input('Do you approve deployment to UAT')
        stage ('UAT') {
            when {
                expression {
                    return (params.Environment ==~ /(?i)(QA|UAT|PROD)/)
                }
            }
            node {
                echo 'Deploy to UAT.'
            }
        }
    }
}